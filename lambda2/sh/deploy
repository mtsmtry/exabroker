npm run pack --prefix ../lib
cp -f ../lib/typeorm-0.2.29.tgz .
cp -f ../lib/exabroker-lib-1.0.0.tgz .
tsc

export tag=0.1
export localRep=exabroker-sync:$tag
export remoteRep=203303191911.dkr.ecr.ap-northeast-1.amazonaws.com/exabroker-sync:$tag
export lambdaName=exabroker-sync

docker build --tag $localRep .

docker tag $localRep $remoteRep
docker push $remoteRep

aws lambda update-function-code --function-name $lambdaName --image-uri $remoteRep